<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="property_facility_view_list" model="ir.ui.view">
        <field name="name">property.facility.view.list</field>
        <field name="model">property.facility</field>
        <field name="arch" type="xml">
            <list string="list view" edit="True" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>
        <record id="property_facility_view_form" model="ir.ui.view">
        <field name="name">property.facility.view.form</field>
        <field name="model">property.facility</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="property_facility_action" model="ir.actions.act_window">
        <field name="name">Facility</field>
        <field name="res_model">property.facility</field>
        <field name="view_mode">list</field>
    </record>
</odoo>