<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="my_sequence_id" model="ir.sequence">
      <field name="name">OP sequence</field>
      <field name="code">clinic.op.reg</field>
      <field name="prefix">OPT</field>
      <field name="padding">3</field>
      <field name="number_next">1</field>
      <field name="company_id" eval="False"/>
    </record>
    <record id="patient_sequence_id" model="ir.sequence">
      <field name="name">Patient sequence</field>
      <field name="code">clinic.test</field>
      <field name="prefix">PA</field>
      <field name="padding">3</field>
      <field name="number_next">1</field>
      <field name="company_id" eval="False"/>
    </record>
    <record id="consult_sequence_id" model="ir.sequence">
      <field name="name">consult sequence</field>
      <field name="code">clinic.consultation</field>
      <field name="prefix">CON</field>
      <field name="padding">3</field>
      <field name="number_next">1</field>
      <field name="company_id" eval="False"/>
    </record>
    <record id="priscription_sequence_id" model="ir.sequence">
      <field name="name">prisccription sequence</field>
      <field name="code">clinic.priscription</field>
      <field name="prefix">PRI</field>
      <field name="padding">5</field>
      <field name="number_next">10</field>
      <field name="company_id" eval="False"/>
    </record>
  </data>
</odoo>